<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net461</TargetFramework>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>MultiRPCIcon.ico</ApplicationIcon>
    <LangVersion>8.0</LangVersion>
    <ApplicationManifest>Properties\app.manifest</ApplicationManifest>
    <StartupObject />
  </PropertyGroup>

  <ItemGroup>
    <None Remove="MultiRPCIcon.ico" />
  </ItemGroup>

  <ItemGroup>
    <Page Remove="Icons.xaml" />
  </ItemGroup>

  <ItemGroup>
    <Compile Include="..\ext\ResourceChangeEvent\ResourceChangeEventArgs.cs" Link="ext\ResourceChangeEventArgs.cs" />
    <Compile Include="..\ext\ResourceChangeEvent\ResourceChangeEventBehavior.cs" Link="ext\ResourceChangeEventBehavior.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Xaml.Behaviors.Wpf" Version="1.1.19" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\MultiRPC.Assets\MultiRPC.Assets.csproj" />
    <ProjectReference Include="..\MultiRPC.Core\MultiRPC.Core.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="MultiRPCIcon.ico" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="ext\" />
  </ItemGroup>

</Project>