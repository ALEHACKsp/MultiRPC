<Window x:Name="Window" x:Class="MultiRPC.GUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MultiRPC.GUI"
        mc:Ignorable="d"
        Title="MultiRPC" Height="500" Width="640" Background="{DynamicResource Brush_MainBackground}" ScrollViewer.VerticalScrollBarVisibility="Disabled" ResizeMode="CanMinimize" WindowStartupLocation="CenterScreen" Icon="/MultiRPC;component/MultiRPC-Icon.ico" Left="Auto" ContentRendered="Window_ContentRendered">
    <Window.Resources>
        <SolidColorBrush x:Key="TextBackground" Color="#FF484B51"/>
    </Window.Resources>
    <Grid x:Name="BackMain" Margin="0,0,0,0" Height="460" Width="620">
        <Button x:Name="BtnToggleRPC" Content="Start MultiRPC" HorizontalAlignment="Left" Height="40" Margin="10,10,0,0" VerticalAlignment="Top" Width="130" BorderBrush="{DynamicResource Brush_Ok}" Foreground="{DynamicResource Brush_Ok}" Background="{DynamicResource Brush_TabBackground}" Style="{DynamicResource ButtonHover}" Click="BtnToggleRPC_Click" Tag="default"/>
        <Label x:Name="LabelStatus" Content="Stat:" HorizontalAlignment="Left" Margin="145,10,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="35" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center"/>
        <Label x:Name="LabelUser" Content="User:" HorizontalAlignment="Left" Margin="145,40,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="35" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center"/>
        <Frame x:Name="ViewLiveRPC" HorizontalAlignment="Left" Height="85" Margin="385,10,0,0" VerticalAlignment="Top" Width="225" Background="{DynamicResource Brush_RPCBack}" Foreground="{x:Null}" NavigationUIVisibility="Hidden"/>
        <Label x:Name="TextStatus" Content="Disconnected" HorizontalAlignment="Left" Margin="185,10,0,0" VerticalAlignment="Top" Foreground="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}" Width="145" Height="25" VerticalContentAlignment="Center"/>
        <Label x:Name="TextUser" Content="" HorizontalAlignment="Left" Margin="185,40,0,0" VerticalAlignment="Top" Foreground="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}" Width="145" Height="25" VerticalContentAlignment="Center"/>
        <Button x:Name="BtnAfk" Content="AFK" HorizontalAlignment="Left" Height="40" Margin="335,55,0,0" VerticalAlignment="Top" Width="45" BorderBrush="{DynamicResource Brush_Button}" Foreground="{DynamicResource Brush_Button}" Background="#FF3C3C3C" Style="{DynamicResource ButtonHover}" Click="BtnAfk_Click"/>
        <TextBox x:Name="TextAfk" MaxLength="25" HorizontalAlignment="Left" Height="25" Margin="145,70,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="185" Foreground="#FFD1D1D1" Background="#FF484B51" VerticalContentAlignment="Center" BorderBrush="{x:Null}" />
        <TabControl x:Name="Menu" HorizontalAlignment="Left" Height="360" Margin="0,100,0,0" VerticalAlignment="Top" Width="620" Background="{DynamicResource Brush_TabBackground}" BorderBrush="{DynamicResource Brush_LabelText}" ScrollViewer.VerticalScrollBarVisibility="Disabled" SelectionChanged="Menu_SelectionChanged" BorderThickness="0,1,0,0">
            <TabItem x:Name="TabMultiRPC" Header="MultiRPC" Style="{DynamicResource TabItemStyle}">
                <Grid x:Name="BackMultiRPC" Background="{DynamicResource Brush_TabBackground}">
                    <Label x:Name="LabelDefaultText1" Content="Text 1:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="70" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" RenderTransformOrigin="0.957,0.92"/>
                    <Frame x:Name="ViewDefaultRPC" HorizontalAlignment="Left" Height="85" Margin="262,33,0,0" VerticalAlignment="Top" Width="225" Background="{DynamicResource Brush_RPCBack}" Foreground="{x:Null}" NavigationUIVisibility="Hidden"/>
                    <Label x:Name="LabelDefaultText2" Content="Text 2:" HorizontalAlignment="Left" Margin="10,40,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="70" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center"/>
                    <Label x:Name="LabelDefaultLargeKey" Content="Large Key:" HorizontalAlignment="Left" Margin="10,70,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="70" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center"/>
                    <Label x:Name="LabelDefaultLargeText" Content="Large Text:" HorizontalAlignment="Left" Margin="10,100,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="70" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center"/>
                    <Label x:Name="LabelDefaultSmallKey" Content="Small Key:" HorizontalAlignment="Left" Margin="10,130,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="70" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center"/>
                    <Label x:Name="LabelDefaultSmallText" Content="Small Text:" HorizontalAlignment="Left" Margin="10,160,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="70" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center"/>
                    <TextBox x:Name="TextDefaultText1" MaxLength="25" HorizontalAlignment="Left" Height="25" Margin="85,10,0,0" TextWrapping="Wrap" Text="Hello" VerticalAlignment="Top" Width="172" Foreground="#FFD1D1D1" Background="#FF484B51" VerticalContentAlignment="Center" BorderBrush="{x:Null}" TextChanged="Default_TextChanged" />
                    <TextBox x:Name="TextDefaultText2" MaxLength="25" HorizontalAlignment="Left" Height="25" Margin="85,40,0,0" TextWrapping="Wrap" Text="World" VerticalAlignment="Top" Width="172" Foreground="#FFD1D1D1" Background="#FF484B51" VerticalContentAlignment="Center" BorderBrush="{x:Null}" TextChanged="Default_TextChanged" />
                    <TextBox x:Name="TextDefaultLarge" MaxLength="25" HorizontalAlignment="Left" Height="25" Margin="85,100,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="172" Foreground="#FFD1D1D1" Background="#FF484B51" VerticalContentAlignment="Center" BorderBrush="{x:Null}" TextChanged="Default_TextChanged" />
                    <TextBox x:Name="TextDefaultSmall" MaxLength="25" HorizontalAlignment="Left" Height="25" Margin="85,160,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="172" Foreground="#FFD1D1D1" Background="#FF484B51" VerticalContentAlignment="Center" BorderBrush="{x:Null}" TextChanged="Default_TextChanged" />
                    <ComboBox x:Name="ItemsDefaultLarge" HorizontalAlignment="Left" Height="25" Margin="85,70,0,0" VerticalAlignment="Top" Width="172" Background="{DynamicResource TextBackground}" BorderBrush="{x:Null}" Style="{DynamicResource ComboBoxStyle}" Foreground="#FFD1D1D1" SelectionChanged="Items_SelectionChanged" Tag="large">
                        <ComboBoxItem Content="" Background="#FFB6B6B6"/>
                    </ComboBox>
                    <ComboBox x:Name="ItemsDefaultSmall" HorizontalAlignment="Left" Height="25" Margin="85,130,0,0" VerticalAlignment="Top" Width="172" Background="{DynamicResource TextBackground}" BorderBrush="{x:Null}" Style="{DynamicResource ComboBoxStyle}" Foreground="#FFD1D1D1" SelectionChanged="Items_SelectionChanged" Tag="small">
                        <ComboBoxItem Content="" Background="#FFB6B6B6"/>
                    </ComboBox>
                    <Label x:Name="TitleDefaultRPC" Content="   MultiRPC Example" HorizontalAlignment="Left" Margin="262,10,0,0" VerticalAlignment="Top" Width="220" Height="23" Foreground="White" FontWeight="Bold" FontSize="11"/>
                    <CheckBox x:Name="ToggleDefaultTime" Content="CheckBox" HorizontalAlignment="Left" Height="15" Margin="135,195,0,0" VerticalAlignment="Top" Width="15" Background="{DynamicResource Brush_LabelText}"/>
                    <Label x:Name="LabelDefaultTime" Content="Show Elapsed Time:" HorizontalAlignment="Left" Margin="10,190,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="120" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" RenderTransformOrigin="0.957,0.92"/>
                </Grid>
            </TabItem>
            <TabItem x:Name="TabCustom" Header="Custom" Style="{DynamicResource TabItemStyle}">
                <Grid x:Name="BackCustom" Background="{DynamicResource Brush_TabBackground}">
                    <Label x:Name="LabelCustomText1" Content="Text 1:" HorizontalAlignment="Left" Margin="10,75,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="70" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" RenderTransformOrigin="0.957,0.92"/>
                    <TextBox x:Name="TextCustomText1" MaxLength="25" HorizontalAlignment="Left" Height="25" Margin="85,75,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="172" Foreground="#FFD1D1D1" Background="#FF484B51" VerticalContentAlignment="Center" BorderBrush="{x:Null}" />
                    <TextBox x:Name="TextCustomClientID" MaxLength="15" HorizontalAlignment="Left" Height="40" Margin="10,30,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="247" Foreground="#FFD1D1D1" Background="#FF484B51" VerticalContentAlignment="Center" BorderBrush="{x:Null}" HorizontalContentAlignment="Center" TextChanged="TextCustomClientID_TextChanged" />
                    <Image x:Name="Help_Error" HorizontalAlignment="Left" Height="30" Margin="227,30,0,0" VerticalAlignment="Top" Width="30" MouseLeftButtonDown="HelpButton_Click" Opacity="0.7" ToolTip="Invalid client ID" IsEnabled="True" Source="../Resources/ExitIcon.png" Visibility="Hidden"/>
                    <Label x:Name="LabelCustomText2" Content="Text 2:" HorizontalAlignment="Left" Margin="10,105,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="70" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" RenderTransformOrigin="0.957,0.92"/>
                    <TextBox x:Name="TextCustomText2" MaxLength="25" HorizontalAlignment="Left" Height="25" Margin="85,105,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="172" Foreground="#FFD1D1D1" Background="#FF484B51" VerticalContentAlignment="Center" BorderBrush="{x:Null}" />
                    <Label x:Name="LabelCustomLargeKey" Content="Large Key:" HorizontalAlignment="Left" Margin="10,135,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="70" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" RenderTransformOrigin="0.957,0.92"/>
                    <TextBox x:Name="TextCustomLargeKey" MaxLength="25" HorizontalAlignment="Left" Height="25" Margin="85,135,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="172" Foreground="#FFD1D1D1" Background="#FF484B51" VerticalContentAlignment="Center" BorderBrush="{x:Null}" />
                    <Label x:Name="LabelCustomLargeText" Content="Large Text:" HorizontalAlignment="Left" Margin="10,165,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="70" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" RenderTransformOrigin="0.957,0.92"/>
                    <TextBox x:Name="TextCustomLargeText" MaxLength="25" HorizontalAlignment="Left" Height="25" Margin="85,165,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="172" Foreground="#FFD1D1D1" Background="#FF484B51" VerticalContentAlignment="Center" BorderBrush="{x:Null}" />
                    <Label x:Name="LabelCustomSmallKey" Content="Small Key:" HorizontalAlignment="Left" Margin="10,195,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="70" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" RenderTransformOrigin="0.957,0.92"/>
                    <TextBox x:Name="TextCustomSmallKey" MaxLength="25" HorizontalAlignment="Left" Height="25" Margin="85,195,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="172" Foreground="#FFD1D1D1" Background="#FF484B51" VerticalContentAlignment="Center" BorderBrush="{x:Null}" />
                    <Label x:Name="LabelCustomSmallText" Content="Small Text:" HorizontalAlignment="Left" Margin="10,225,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="70" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" RenderTransformOrigin="0.957,0.92"/>
                    <TextBox x:Name="TextCustomSmallText" MaxLength="25" HorizontalAlignment="Left" Height="25" Margin="85,225,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="172" Foreground="#FFD1D1D1" Background="#FF484B51" VerticalContentAlignment="Center" BorderBrush="{x:Null}" />
                    <Image x:Name="HelpClientID" HorizontalAlignment="Left" Height="25" Margin="257,35,0,0" VerticalAlignment="Top" Width="23" Source="../Resources/HelpIcon.png" MouseLeftButtonDown="HelpButton_Click" Opacity="0.7"/>
                    <Image x:Name="HelpText1" HorizontalAlignment="Left" Height="25" Margin="257,75,0,0" VerticalAlignment="Top" Width="23" Source="../Resources/HelpIcon.png" MouseLeftButtonDown="HelpButton_Click" Opacity="0.7"/>
                    <Image x:Name="HelpText2" HorizontalAlignment="Left" Height="25" Margin="257,105,0,0" VerticalAlignment="Top" Width="23" Source="../Resources/HelpIcon.png" MouseLeftButtonDown="HelpButton_Click" Opacity="0.7"/>
                    <Image x:Name="HelpLargeKey" HorizontalAlignment="Left" Height="25" Margin="257,135,0,0" VerticalAlignment="Top" Width="23" Source="../Resources/HelpIcon.png" MouseLeftButtonDown="HelpButton_Click" Opacity="0.7"/>
                    <Image x:Name="HelpSmallText" HorizontalAlignment="Left" Height="25" Margin="257,225,0,0" VerticalAlignment="Top" Width="23" Source="../Resources/HelpIcon.png" MouseLeftButtonDown="HelpButton_Click" Opacity="0.7"/>
                    <Image x:Name="HelpLargeText" HorizontalAlignment="Left" Height="25" Margin="257,165,0,0" VerticalAlignment="Top" Width="23" Source="../Resources/HelpIcon.png" MouseLeftButtonDown="HelpButton_Click" Opacity="0.7"/>
                    <Image x:Name="HelpSmallKey" HorizontalAlignment="Left" Height="25" Margin="257,195,0,0" VerticalAlignment="Top" Width="23" Source="../Resources/HelpIcon.png" MouseLeftButtonDown="HelpButton_Click" Opacity="0.7"/>
                    <Image x:Name="ImageHelp" HorizontalAlignment="Left" Height="220" Margin="280,30,0,0" VerticalAlignment="Top" Width="326"/>
                    <Label x:Name="LabelCustomTime" Content="Show Elapsed Time:" HorizontalAlignment="Left" Margin="10,255,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="120" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" RenderTransformOrigin="0.957,0.92"/>
                    <CheckBox x:Name="ToggleCustomTime" Content="CheckBox" HorizontalAlignment="Left" Height="15" Margin="135,261,0,0" VerticalAlignment="Top" Width="15" Background="{DynamicResource Brush_LabelText}"/>
                    <Image x:Name="BtnAddProfile" HorizontalAlignment="Left" Height="25" Margin="551,0,0,0" VerticalAlignment="Top" Width="25" Source="../Resources/AddIcon.png"/>
                    <Image x:Name="BtnShareProfile" HorizontalAlignment="Left" Height="25" Margin="521,0,0,0" VerticalAlignment="Top" Width="25" Source="../Resources/ShareIcon.png"/>
                    <Image x:Name="BtnDeleteProfile" HorizontalAlignment="Left" Height="25" Margin="581,0,0,0" VerticalAlignment="Top" Width="25" Source="../Resources/DeleteIcon.png"/>
                    <ToolBar x:Name="MenuCustomProfiles" HorizontalAlignment="Left" Height="25" VerticalAlignment="Top" Width="516" Background="{DynamicResource TabItem.Disabled.Background}" BorderBrush="{DynamicResource TabItem.Disabled.Background}" UseLayoutRounding="False" ScrollViewer.VerticalScrollBarVisibility="Disabled">
                        <Button Content="Test" Background="#FF606060" Foreground="White" BorderBrush="#FF606060" Padding="10,1" Margin="5,0"/>
                        <Button Content="Test2" BorderBrush="#FF606060" Background="#FF606060" Foreground="White" Padding="10,1" Margin="5,0"/>
                        <Button Content="Test2" BorderBrush="#FF606060" Background="#FF606060" Foreground="White" Padding="10,1" Margin="5,0"/>
                        <Button Content="Test2" BorderBrush="#FF606060" Background="#FF606060" Foreground="White" Padding="10,1" Margin="5,0"/>
                        <Button Content="Test2" BorderBrush="#FF606060" Background="#FF606060" Foreground="White" Padding="10,1" Margin="5,0"/>
                        <Button Content="Test2" BorderBrush="#FF606060" Background="#FF606060" Foreground="White" Padding="10,1" Margin="5,0"/>
                        <Button Content="Test2" BorderBrush="#FF606060" Background="#FF606060" Foreground="White" Padding="10,1" Margin="5,0"/>
                        <Button Content="Test2" BorderBrush="#FF606060" Background="#FF606060" Foreground="White" Padding="10,1" Margin="5,0"/>
                        <Button Content="Test2" BorderBrush="#FF606060" Background="#FF606060" Foreground="White" Padding="10,1" Margin="5,0"/>
                        <Button Content="Test2" BorderBrush="#FF606060" Background="#FF606060" Foreground="White" Padding="10,1" Margin="5,0"/>
                    </ToolBar>
                </Grid>
            </TabItem>
            <TabItem x:Name="TabPrograms" Header="Programs" Style="{DynamicResource TabItemStyle}" IsEnabled="False">
                <Grid x:Name="BackPrograms" Background="{DynamicResource Brush_TabBackground}"/>
            </TabItem>
            <TabItem x:Name="TabSettings" Header="Settings" Style="{DynamicResource TabItemStyle}">
                <Grid x:Name="BackSettings" Background="{DynamicResource Brush_TabBackground}" Height="333">
                    <Image x:Name="LinkWebsite" HorizontalAlignment="Left" Height="60" Margin="10,48,0,0" VerticalAlignment="Top" Width="60" Source="../Resources/WebsiteIcon.png" MouseLeftButtonDown="Links_Clicked"/>
                    <Label x:Name="LabelStatus_Copy" Content="Website" HorizontalAlignment="Left" Margin="10,23,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="60" Height="25" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontWeight="Medium" FontSize="11"/>
                    <Image x:Name="LinkDownload" HorizontalAlignment="Left" Height="60" Margin="75,48,0,0" VerticalAlignment="Top" Width="60" Source="../Resources/DownloadIcon.png" MouseLeftButtonDown="Links_Clicked"/>
                    <Image x:Name="LinkGithub" HorizontalAlignment="Left" Height="60" Margin="140,48,0,0" VerticalAlignment="Top" Width="60" Source="../Resources/GithubIcon.png" MouseLeftButtonDown="Links_Clicked"/>
                    <Image x:Name="LinkServer" HorizontalAlignment="Left" Height="60" Margin="280,48,0,0" VerticalAlignment="Top" Width="60" Source="../Resources/ServerIcon.jpg" MouseLeftButtonDown="Links_Clicked"/>
                    <Image x:Name="LinkDGithub" HorizontalAlignment="Left" Height="60" Margin="546,48,0,0" VerticalAlignment="Top" Width="60" Source="../Resources/GithubIcon.png" MouseLeftButtonDown="Links_Clicked"/>
                    <Image x:Name="LinkDStatus" HorizontalAlignment="Left" Height="60" Margin="481,48,0,0" VerticalAlignment="Top" Width="60" Source="../Resources/DiscordIcon.png" MouseLeftButtonDown="Links_Clicked"/>
                    <Image  x:Name="LinkDWebsite" HorizontalAlignment="Left" Height="60" Margin="416,48,0,0" VerticalAlignment="Top" Width="60" Source="../Resources/DiscordIcon.png" MouseLeftButtonDown="Links_Clicked"/>
                    <Label x:Name="TitleMultiRPC" Content="MultiRPC" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Top" Width="190" Height="23" Foreground="White" FontWeight="Bold" FontSize="11" HorizontalContentAlignment="Center"/>
                    <Label x:Name="LabelStatus_Copy1" Content="Download" HorizontalAlignment="Left" Margin="75,23,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="60" Height="25" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontWeight="Medium" FontSize="11"/>
                    <Label x:Name="LabelStatus_Copy2" Content="Github" HorizontalAlignment="Left" Margin="140,23,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="60" Height="25" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontWeight="Medium" FontSize="11"/>
                    <Label x:Name="LabelStatus_Copy3" Content="Website" HorizontalAlignment="Left" Margin="416,23,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="60" Height="25" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontWeight="Medium" FontSize="11"/>
                    <Label x:Name="TitleDiscord" Content="Discord" HorizontalAlignment="Left" Margin="416,0,0,0" VerticalAlignment="Top" Width="190" Height="23" Foreground="White" FontWeight="Bold" FontSize="11" HorizontalContentAlignment="Center"/>
                    <Label x:Name="LabelStatus_Copy4" Content="Status" HorizontalAlignment="Left" Margin="481,23,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="60" Height="25" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontWeight="Medium" FontSize="11"/>
                    <Label x:Name="LabelStatus_Copy5" Content="C# Github" HorizontalAlignment="Left" Margin="546,23,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="60" Height="25" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontWeight="Medium" FontSize="11"/>
                    <Label x:Name="TitleDefaultRPC_Copy2" Content="Discord Server" HorizontalAlignment="Left" Margin="205,20,0,0" VerticalAlignment="Top" Width="206" Height="23" Foreground="White" FontWeight="Bold" FontSize="11" HorizontalContentAlignment="Center"/>
                    <Label x:Name="LabelPipe" Content="Pipe:" HorizontalAlignment="Left" Margin="10,141,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="70" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" ToolTip="Set to 2 if you have multiple Discord clients open"/>
                    <Label x:Name="TitleSettings" Content="Settings" HorizontalAlignment="Left" Margin="10,113,0,0" VerticalAlignment="Top" Width="190" Height="23" Foreground="White" FontWeight="Bold" FontSize="11" HorizontalContentAlignment="Center"/>
                    <Label x:Name="LabelVersion" Content="Version:" HorizontalAlignment="Left" Margin="10,298,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="70" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center"/>
                    <Label x:Name="LabelAutoStart" Content="Auto Start:" HorizontalAlignment="Left" Margin="10,171,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="70" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" ToolTip="Auto start RPC when app loads"/>
                    <Label x:Name="TextVersion" Content="0.0.0" HorizontalAlignment="Left" Margin="85,298,0,0" VerticalAlignment="Top" Foreground="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}" Width="90" Height="25" VerticalContentAlignment="Center"/>
                    <ComboBox x:Name="ItemsAutoStart" HorizontalAlignment="Left" SelectedIndex="0" Height="25" Margin="85,171,0,0" VerticalAlignment="Top" Width="70" Style="{DynamicResource ComboBoxStyle}" Foreground="White" SelectionChanged="ItemsAutoStart_SelectionChanged">
                        <ComboBoxItem Content="No" Background="#FFB6B6B6" Foreground="Black"/>
                        <ComboBoxItem Content="MultiRPC" Background="#FFB6B6B6" Foreground="Black"/>
                        <ComboBoxItem Content="Custom" Background="#FFB6B6B6" Foreground="Black"/>
                    </ComboBox>
                    <CheckBox x:Name="ToggleAfkTime" Content="CheckBox" HorizontalAlignment="Left" Height="15" Margin="85,207,0,0" VerticalAlignment="Top" Width="15" Background="{DynamicResource Brush_LabelText}" Checked="ToggleAfkTime_Checked" Unchecked="ToggleAfkTime_Checked"/>
                    <Label x:Name="LabelAfkTime" Content="Afk Time:" HorizontalAlignment="Left" Margin="10,201,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="70" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" ToolTip="Enable elapsed time on afk RPC"/>
                    <ComboBox x:Name="ItemsPipe" HorizontalAlignment="Left" SelectedIndex="0" Height="25" Margin="85,141,0,0" VerticalAlignment="Top" Width="70" Style="{DynamicResource ComboBoxStyle}" Foreground="White">
                        <ComboBoxItem Content="1" Background="#FFB6B6B6" Foreground="Black"/>
                        <ComboBoxItem Content="2" Background="#FFB6B6B6" Foreground="Black"/>
                    </ComboBox>
                    <Label x:Name="LabelTokenCheck" Content="Token Check:" HorizontalAlignment="Left" Margin="221,141,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="90" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" ToolTip="Checks for valid token using Discord API"/>
                    <Label x:Name="TitleDisable" Content="Disable" HorizontalAlignment="Left" Margin="205,113,0,0" VerticalAlignment="Top" Width="190" Height="23" Foreground="White" FontWeight="Bold" FontSize="11" HorizontalContentAlignment="Center"/>
                    <CheckBox x:Name="ToggleTokenCheck" Content="CheckBox" HorizontalAlignment="Left" Height="15" Margin="316,147,0,0" VerticalAlignment="Top" Width="15" Background="{DynamicResource Brush_LabelText}"/>
                    <Label x:Name="LabelTokenCheck_Copy" Content="Programs Tab:" HorizontalAlignment="Left" Margin="221,171,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="90" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" ToolTip="Not use datm :/"/>
                    <CheckBox x:Name="ToggleProgramsTab" Content="CheckBox" HorizontalAlignment="Left" Height="15" Margin="316,177,0,0" VerticalAlignment="Top" Width="15" Background="{DynamicResource Brush_LabelText}" Checked="ToggleProgramsTab_Checked" Unchecked="ToggleProgramsTab_Checked"/>
                    <Label x:Name="LabelTokenCheck_Copy1" Content="Help Icons:" HorizontalAlignment="Left" Margin="221,201,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="90" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" ToolTip="Removes help icons from custom tab"/>
                    <CheckBox x:Name="ToggleHelpIcons" Content="CheckBox" HorizontalAlignment="Left" Height="15" Margin="316,207,0,0" VerticalAlignment="Top" Width="15" Background="{DynamicResource Brush_LabelText}" Checked="ToggleHelpIcons_Checked" Unchecked="ToggleHelpIcons_Checked"/>
                    <Label x:Name="LabelMultiProfiles" Content="Multi Profiles:" HorizontalAlignment="Left" Margin="221,231,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="90" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" ToolTip="Removes help icons from custom tab"/>
                    <CheckBox x:Name="ToggleMultiProfiles" Content="CheckBox" HorizontalAlignment="Left" Height="15" Margin="316,237,0,0" VerticalAlignment="Top" Width="15" Background="{DynamicResource Brush_LabelText}" Checked="ToggleMultiProfiles_Checked" Unchecked="ToggleMultiProfiles_Checked"/>
                </Grid>
            </TabItem>
            <TabItem x:Name="TabLogs" Header="Logs" Style="{DynamicResource TabItemStyle}">
                <Grid x:Name="BackLogs" Background="{DynamicResource Brush_TabBackground}">
                    <TextBox x:Name="Logs" x:FieldModifier="public" IsReadOnly="True" TextWrapping="Wrap" Background="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" BorderBrush="{x:Null}" Foreground="#FFC9C9C9" Text="[App] Hello this is a console log" BorderThickness="0" HorizontalScrollBarVisibility="Auto" Height="332" VerticalAlignment="Top"/>
                </Grid>
            </TabItem>
        </TabControl>
        <Button x:Name="BtnUpdatePresence" Content="Update Presence" HorizontalAlignment="Left" Height="40" Margin="10,55,0,0" VerticalAlignment="Top" Width="130" BorderBrush="{DynamicResource Brush_RPCBack}" Foreground="White" Background="{DynamicResource Brush_Button}" Style="{DynamicResource ButtonHover}" Click="BtnUpdatePresence_Click" IsEnabled="False"/>
        <Button x:Name="BtnAuto" Content="Auto" HorizontalAlignment="Left" Height="40" Margin="335,0,0,410" VerticalAlignment="Bottom" Width="45" BorderBrush="{DynamicResource Brush_No}" Foreground="{DynamicResource Brush_No}" Background="{DynamicResource Brush_TabBackground}" Style="{DynamicResource ButtonHover}" Click="BtnAuto_Click" IsEnabled="False"/>

    </Grid>
</Window>
