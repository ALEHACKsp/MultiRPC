<Window x:Name="Window" x:Class="MultiRPC.GUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MultiRPC.GUI"
        mc:Ignorable="d"
        Title="MultiRPC" Height="500" Width="640" Background="{DynamicResource Brush_MainBackground}" ScrollViewer.VerticalScrollBarVisibility="Disabled" ResizeMode="CanMinimize" WindowStartupLocation="CenterScreen" Icon="/MultiRPC;component/MultiRPC-Icon.ico" Left="Auto">
    <Window.Resources>
        <SolidColorBrush x:Key="TextBackground" Color="#FF484B51"/>
    </Window.Resources>
    <Grid x:Name="BackMain" Margin="0,0,0,0" Height="460" Width="625">
        <Button x:Name="BtnToggleRPC" Content="Start MultiRPC" HorizontalAlignment="Left" Height="40" Margin="10,10,0,0" VerticalAlignment="Top" Width="130" BorderBrush="{DynamicResource Brush_Ok}" Foreground="{DynamicResource Brush_Ok}" Background="{DynamicResource Brush_TabBackground}" Style="{DynamicResource ButtonHover}" Click="BtnToggleRPC_Click"/>
        <Label x:Name="LabelStatus" Content="Stat:" HorizontalAlignment="Left" Margin="145,10,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="35" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center"/>
        <Label x:Name="LabelUser" Content="User:" HorizontalAlignment="Left" Margin="145,40,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="35" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center"/>
        <Frame x:Name="ViewLiveRPC" HorizontalAlignment="Left" Height="85" Margin="390,10,0,0" VerticalAlignment="Top" Width="225" Background="{DynamicResource Brush_RPCBack}" Foreground="{x:Null}" NavigationUIVisibility="Hidden"/>
        <Label x:Name="TextStatus" Content="Disconnected" HorizontalAlignment="Left" Margin="185,10,0,0" VerticalAlignment="Top" Foreground="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}" Width="145" Height="25" VerticalContentAlignment="Center"/>
        <Label x:Name="TextUser" Content="" HorizontalAlignment="Left" Margin="185,40,0,0" VerticalAlignment="Top" Foreground="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}" Width="145" Height="25" VerticalContentAlignment="Center"/>
        <Button x:Name="BtnAfk" Content="AFK" HorizontalAlignment="Left" Height="40" Margin="335,55,0,0" VerticalAlignment="Top" Width="50" BorderBrush="{DynamicResource Brush_Button}" Foreground="{DynamicResource Brush_Button}" Background="#FF3C3C3C" Style="{DynamicResource ButtonHover}" Click="BtnAfk_Click"/>
        <TextBox x:Name="TextAfk" MaxLength="25" HorizontalAlignment="Left" Height="25" Margin="145,70,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="185" Foreground="#FFD1D1D1" Background="#FF484B51" VerticalContentAlignment="Center" BorderBrush="{x:Null}" />
        <TabControl x:Name="Menu" HorizontalAlignment="Left" Height="360" Margin="0,100,0,0" VerticalAlignment="Top" Width="625" Background="{DynamicResource Brush_TabBackground}" BorderBrush="{DynamicResource Brush_LabelText}" ScrollViewer.VerticalScrollBarVisibility="Disabled" SelectionChanged="Menu_SelectionChanged">
            <TabItem x:Name="TabMultiRPC" Header="MultiRPC" Style="{DynamicResource TabItemStyle}">
                <Grid x:Name="BackMultiRPC" Background="{DynamicResource Brush_TabBackground}">
                    <Label x:Name="LabelDefaultText1" Content="Text 1:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="70" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" RenderTransformOrigin="0.957,0.92"/>
                    <Frame x:Name="ViewDefaultRPC" HorizontalAlignment="Left" Height="85" Margin="262,33,0,0" VerticalAlignment="Top" Width="225" Background="{DynamicResource Brush_RPCBack}" Foreground="{x:Null}" NavigationUIVisibility="Hidden"/>
                    <Label x:Name="LabelDefaultText2" Content="Text 2:" HorizontalAlignment="Left" Margin="10,40,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="70" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center"/>
                    <Label x:Name="LabelDefaultLargeKey" Content="Large Key:" HorizontalAlignment="Left" Margin="10,70,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="70" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center"/>
                    <Label x:Name="LabelDefaultLargeText" Content="Large Text:" HorizontalAlignment="Left" Margin="10,100,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="70" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center"/>
                    <Label x:Name="LabelDefaultSmallKey" Content="Small Key:" HorizontalAlignment="Left" Margin="10,130,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="70" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center"/>
                    <Label x:Name="LabelDefaultSmallText" Content="Small Text:" HorizontalAlignment="Left" Margin="10,160,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="70" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center"/>
                    <TextBox x:Name="TextDefaultText1" MaxLength="25" HorizontalAlignment="Left" Height="25" Margin="85,10,0,0" TextWrapping="Wrap" Text="Hello" VerticalAlignment="Top" Width="172" Foreground="#FFD1D1D1" Background="#FF484B51" VerticalContentAlignment="Center" BorderBrush="{x:Null}" TextChanged="Default_TextChanged" />
                    <TextBox x:Name="TextDefaultText2" MaxLength="25" HorizontalAlignment="Left" Height="25" Margin="85,40,0,0" TextWrapping="Wrap" Text="World" VerticalAlignment="Top" Width="172" Foreground="#FFD1D1D1" Background="#FF484B51" VerticalContentAlignment="Center" BorderBrush="{x:Null}" TextChanged="Default_TextChanged" />
                    <TextBox x:Name="TextDefaultLarge" MaxLength="25" HorizontalAlignment="Left" Height="25" Margin="85,100,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="172" Foreground="#FFD1D1D1" Background="#FF484B51" VerticalContentAlignment="Center" BorderBrush="{x:Null}" TextChanged="Default_TextChanged" />
                    <TextBox x:Name="TextDefaultSmall" MaxLength="25" HorizontalAlignment="Left" Height="25" Margin="85,160,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="172" Foreground="#FFD1D1D1" Background="#FF484B51" VerticalContentAlignment="Center" BorderBrush="{x:Null}" TextChanged="Default_TextChanged" />
                    <ComboBox x:Name="ItemsDefaultLarge" HorizontalAlignment="Left" Height="25" Margin="85,70,0,0" VerticalAlignment="Top" Width="172" Background="{DynamicResource TextBackground}" BorderBrush="{x:Null}" Style="{DynamicResource ComboBoxStyle}" Foreground="#FFD1D1D1" SelectionChanged="Items_SelectionChanged" Tag="large" Loaded="ItemsDefaultLarge_Loaded">
                        <ComboBoxItem Content="" Background="#FFB6B6B6"/>
                    </ComboBox>
                    <ComboBox x:Name="ItemsDefaultSmall" HorizontalAlignment="Left" Height="25" Margin="85,130,0,0" VerticalAlignment="Top" Width="172" Background="{DynamicResource TextBackground}" BorderBrush="{x:Null}" Style="{DynamicResource ComboBoxStyle}" Foreground="#FFD1D1D1" SelectionChanged="Items_SelectionChanged" Tag="small" Loaded="ItemsDefaultSmall_Loaded">
                        <ComboBoxItem Content="" Background="#FFB6B6B6"/>
                    </ComboBox>
                    <Label x:Name="TitleDefaultRPC" Content="   MultiRPC Example" HorizontalAlignment="Left" Margin="262,10,0,0" VerticalAlignment="Top" Width="220" Height="23" Foreground="White" FontWeight="Bold" FontSize="11"/>
                    <CheckBox x:Name="ToggleDefaultTime" Content="CheckBox" HorizontalAlignment="Left" Height="15" Margin="135,195,0,0" VerticalAlignment="Top" Width="15" Background="{DynamicResource Brush_LabelText}"/>
                    <Label x:Name="LabelDefaultTime" Content="Show Elapsed Time:" HorizontalAlignment="Left" Margin="10,190,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="120" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" RenderTransformOrigin="0.957,0.92"/>
                </Grid>
            </TabItem>
            <TabItem x:Name="TabCustom" Header="Custom" Style="{DynamicResource TabItemStyle}">
                <Grid x:Name="BackCustom" Background="{DynamicResource Brush_TabBackground}">
                    <Label x:Name="LabelCustomText1" Content="Text 1:" HorizontalAlignment="Left" Margin="10,56,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="70" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" RenderTransformOrigin="0.957,0.92"/>
                    <TextBox x:Name="TextCustomText1" MaxLength="25" HorizontalAlignment="Left" Height="25" Margin="85,55,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="172" Foreground="#FFD1D1D1" Background="#FF484B51" VerticalContentAlignment="Center" BorderBrush="{x:Null}" />
                    <TextBox x:Name="TextCustomClientID" MaxLength="15" HorizontalAlignment="Left" Height="40" Margin="10,10,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="247" Foreground="#FFD1D1D1" Background="#FF484B51" VerticalContentAlignment="Center" BorderBrush="{x:Null}" HorizontalContentAlignment="Center" TextChanged="TextCustomClientID_TextChanged" />
                    <Image x:Name="Help_Error" HorizontalAlignment="Left" Height="30" Margin="227,10,0,0" VerticalAlignment="Top" Width="30" MouseLeftButtonDown="HelpButton_Click" Opacity="0.7" ToolTip="Invalid client ID" Visibility="Hidden" IsEnabled="True" Source="../Resources/ExitIcon.png"/>
                    <Label x:Name="LabelCustomText2" Content="Text 2:" HorizontalAlignment="Left" Margin="10,86,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="70" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" RenderTransformOrigin="0.957,0.92"/>
                    <TextBox x:Name="TextCustomText2" MaxLength="25" HorizontalAlignment="Left" Height="25" Margin="85,85,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="172" Foreground="#FFD1D1D1" Background="#FF484B51" VerticalContentAlignment="Center" BorderBrush="{x:Null}" />
                    <Label x:Name="LabelCustomLargeKey" Content="Large Key:" HorizontalAlignment="Left" Margin="10,117,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="70" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" RenderTransformOrigin="0.957,0.92"/>
                    <TextBox x:Name="TextCustomLargeKey" MaxLength="25" HorizontalAlignment="Left" Height="25" Margin="85,116,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="172" Foreground="#FFD1D1D1" Background="#FF484B51" VerticalContentAlignment="Center" BorderBrush="{x:Null}" />
                    <Label x:Name="LabelCustomLargeText" Content="Large Text:" HorizontalAlignment="Left" Margin="10,147,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="70" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" RenderTransformOrigin="0.957,0.92"/>
                    <TextBox x:Name="TextCustomLargeText" MaxLength="25" HorizontalAlignment="Left" Height="25" Margin="85,146,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="172" Foreground="#FFD1D1D1" Background="#FF484B51" VerticalContentAlignment="Center" BorderBrush="{x:Null}" />
                    <Label x:Name="LabelCustomSmallKey" Content="Small Key:" HorizontalAlignment="Left" Margin="10,177,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="70" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" RenderTransformOrigin="0.957,0.92"/>
                    <TextBox x:Name="TextCustomSmallKey" MaxLength="25" HorizontalAlignment="Left" Height="25" Margin="85,176,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="172" Foreground="#FFD1D1D1" Background="#FF484B51" VerticalContentAlignment="Center" BorderBrush="{x:Null}" />
                    <Label x:Name="LabelCustomSmallText" Content="Small Text:" HorizontalAlignment="Left" Margin="10,207,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="70" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" RenderTransformOrigin="0.957,0.92"/>
                    <TextBox x:Name="TextCustomSmallText" MaxLength="25" HorizontalAlignment="Left" Height="25" Margin="85,206,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="172" Foreground="#FFD1D1D1" Background="#FF484B51" VerticalContentAlignment="Center" BorderBrush="{x:Null}" />
                    <Image x:Name="HelpClientID" HorizontalAlignment="Left" Height="25" Margin="257,20,0,0" VerticalAlignment="Top" Width="23" Source="../Resources/HelpIcon.png" MouseLeftButtonDown="HelpButton_Click" Opacity="0.7"/>
                    <Image x:Name="HelpText1" HorizontalAlignment="Left" Height="25" Margin="257,56,0,0" VerticalAlignment="Top" Width="23" Source="../Resources/HelpIcon.png" MouseLeftButtonDown="HelpButton_Click" Opacity="0.7"/>
                    <Image x:Name="HelpText2" HorizontalAlignment="Left" Height="25" Margin="257,86,0,0" VerticalAlignment="Top" Width="23" Source="../Resources/HelpIcon.png" MouseLeftButtonDown="HelpButton_Click" Opacity="0.7"/>
                    <Image x:Name="HelpLargeKey" HorizontalAlignment="Left" Height="25" Margin="257,117,0,0" VerticalAlignment="Top" Width="23" Source="../Resources/HelpIcon.png" MouseLeftButtonDown="HelpButton_Click" Opacity="0.7"/>
                    <Image x:Name="HelpSmallText" HorizontalAlignment="Left" Height="25" Margin="257,207,0,0" VerticalAlignment="Top" Width="23" Source="../Resources/HelpIcon.png" MouseLeftButtonDown="HelpButton_Click" Opacity="0.7"/>
                    <Image x:Name="HelpLargeText" HorizontalAlignment="Left" Height="25" Margin="257,147,0,0" VerticalAlignment="Top" Width="23" Source="../Resources/HelpIcon.png" MouseLeftButtonDown="HelpButton_Click" Opacity="0.7"/>
                    <Image x:Name="HelpSmallKey" HorizontalAlignment="Left" Height="25" Margin="257,177,0,0" VerticalAlignment="Top" Width="23" Source="../Resources/HelpIcon.png" MouseLeftButtonDown="HelpButton_Click" Opacity="0.7"/>
                    <Image x:Name="ImageHelp" HorizontalAlignment="Left" Height="221" Margin="280,10,0,0" VerticalAlignment="Top" Width="329"/>
                    <Label x:Name="LabelCustomTime" Content="Show Elapsed Time:" HorizontalAlignment="Left" Margin="10,236,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="120" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" RenderTransformOrigin="0.957,0.92"/>
                    <CheckBox x:Name="ToggleCustomTime" Content="CheckBox" HorizontalAlignment="Left" Height="15" Margin="135,242,0,0" VerticalAlignment="Top" Width="15" Background="{DynamicResource Brush_LabelText}"/>
                </Grid>
            </TabItem>
            <TabItem x:Name="TabPrograms" Header="Programs" Style="{DynamicResource TabItemStyle}" IsEnabled="False">
                <Grid x:Name="BackPrograms" Background="{DynamicResource Brush_TabBackground}"/>
            </TabItem>
            <TabItem x:Name="TabSettings" Header="Settings" Style="{DynamicResource TabItemStyle}">
                <Grid x:Name="BackSettings" Background="{DynamicResource Brush_TabBackground}">
                    <Label x:Name="LabelAfkTime" Content="Show Afk Time:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Foreground="{DynamicResource Brush_LabelText}" Width="120" Height="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" RenderTransformOrigin="0.957,0.92"/>
                    <CheckBox x:Name="ToggleAfkTime" Content="CheckBox" HorizontalAlignment="Left" Height="15" Margin="135,16,0,0" VerticalAlignment="Top" Width="15" Background="{DynamicResource Brush_LabelText}" Checked="ToggleAfkTime_Checked"/>
                </Grid>
            </TabItem>
            <TabItem x:Name="TabLogs" Header="Logs" Style="{DynamicResource TabItemStyle}">
                <Grid x:Name="BackLogs" Background="{DynamicResource Brush_TabBackground}">
                    <TextBox x:Name="Logs" x:FieldModifier="public" IsReadOnly="True" TextWrapping="Wrap" Background="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" BorderBrush="{x:Null}" Foreground="#FFC9C9C9" Text="[App] Hello this is a console log" BorderThickness="0" HorizontalScrollBarVisibility="Auto" Height="332" VerticalAlignment="Top"/>
                </Grid>
            </TabItem>
        </TabControl>
        <Button x:Name="BtnUpdatePresence" Content="Update Presence" HorizontalAlignment="Left" Height="40" Margin="10,55,0,0" VerticalAlignment="Top" Width="130" BorderBrush="{DynamicResource Brush_RPCBack}" Foreground="White" Background="{DynamicResource Brush_Button}" Style="{DynamicResource ButtonHover}" Click="BtnUpdatePresence_Click" IsEnabled="False"/>
        <Button x:Name="BtnAuto" Content="Auto" HorizontalAlignment="Left" Height="40" Margin="335,0,0,410" VerticalAlignment="Bottom" Width="50" BorderBrush="{DynamicResource Brush_No}" Foreground="{DynamicResource Brush_No}" Background="{DynamicResource Brush_TabBackground}" Style="{DynamicResource ButtonHover}" Click="BtnAuto_Click" IsEnabled="False"/>

    </Grid>
</Window>
