<Page x:Class="MultiRPC.GUI.Pages.ProgramsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:MultiRPC.GUI.Pages"
      mc:Ignorable="d"
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ProgramsPage">

    <Grid>
        <Border CornerRadius="10" HorizontalAlignment="Center" VerticalAlignment="Center" Background="#7F000000">
            <StackPanel Margin="10">
                <Image Name="imgProgramsIcon" Source="{DynamicResource ProgramsIconDrawingImage}" Height="150" />
                <TextBlock Text="To be made...." FontSize="40" FontWeight="SemiBold" HorizontalAlignment="Center" />
            </StackPanel>
        </Border>
    </Grid>
</Page>