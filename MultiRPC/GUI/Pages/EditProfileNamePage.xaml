<Page x:Class="MultiRPC.GUI.Pages.EditProfileNamePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:MultiRPC.GUI.Pages"
      mc:Ignorable="d"
      MinHeight="79" MinWidth="250"
      Title="Edit Profile Name">

    <StackPanel Margin="10">
        <TextBox Name="tbNewProfileName" />
        <Button Name="btnDone" Click="ButDone_OnClick" Style="{DynamicResource ButtonGreen}" HorizontalAlignment="Left"
                Margin="0,10,0,0" />
    </StackPanel>
</Page>